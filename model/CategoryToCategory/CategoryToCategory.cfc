<cfcomponent displayname="CategoryToCategory" output="false">		<cfproperty name="SuperCategoryID" type="numeric" default="" />		<cfproperty name="SubCategoryID" type="numeric" default="" />			<!---	PROPERTIES	--->	<cfset variables.instance = StructNew() />	<!---	INITIALIZATION / CONFIGURATION	--->	<cffunction name="init" access="public" returntype="CategoryToCategory" output="false">		<cfargument name="SuperCategoryID" type="string" required="false" default="" />		<cfargument name="SubCategoryID" type="string" required="false" default="" />				<!--- run setters --->		<cfset setSuperCategoryID(arguments.SuperCategoryID) />		<cfset setSubCategoryID(arguments.SubCategoryID) />				<cfreturn this /> 	</cffunction>	<!---	PUBLIC FUNCTIONS	--->	<cffunction name="setMemento" access="public" returntype="CategoryToCategory" output="false">		<cfargument name="memento" type="struct" required="yes"/>		<cfset variables.instance = arguments.memento />		<cfreturn this />	</cffunction>	<cffunction name="getMemento" access="public" returntype="struct" output="false" >		<cfreturn variables.instance />	</cffunction>	<cffunction name="validate" access="public" returntype="array" output="false">		<cfset var errors = arrayNew(1) />		<cfset var thisError = structNew() />				<!--- SuperCategoryID --->		<cfif (NOT len(trim(getSuperCategoryID())))>			<cfset thisError.field = "SuperCategoryID" />			<cfset thisError.type = "required" />			<cfset thisError.message = "SuperCategoryID is required" />			<cfset arrayAppend(errors,duplicate(thisError)) />		</cfif>		<cfif (len(trim(getSuperCategoryID())) AND NOT isNumeric(trim(getSuperCategoryID())))>			<cfset thisError.field = "SuperCategoryID" />			<cfset thisError.type = "invalidType" />			<cfset thisError.message = "SuperCategoryID is not numeric" />			<cfset arrayAppend(errors,duplicate(thisError)) />		</cfif>				<!--- SubCategoryID --->		<cfif (NOT len(trim(getSubCategoryID())))>			<cfset thisError.field = "SubCategoryID" />			<cfset thisError.type = "required" />			<cfset thisError.message = "SubCategoryID is required" />			<cfset arrayAppend(errors,duplicate(thisError)) />		</cfif>		<cfif (len(trim(getSubCategoryID())) AND NOT isNumeric(trim(getSubCategoryID())))>			<cfset thisError.field = "SubCategoryID" />			<cfset thisError.type = "invalidType" />			<cfset thisError.message = "SubCategoryID is not numeric" />			<cfset arrayAppend(errors,duplicate(thisError)) />		</cfif>				<cfreturn errors />	</cffunction>	<!---	ACCESSORS	--->	<cffunction name="setSuperCategoryID" access="public" returntype="void" output="false">		<cfargument name="SuperCategoryID" type="string" required="true" />		<cfset variables.instance.SuperCategoryID = arguments.SuperCategoryID />	</cffunction>	<cffunction name="getSuperCategoryID" access="public" returntype="string" output="false">		<cfreturn variables.instance.SuperCategoryID />	</cffunction>	<cffunction name="setSubCategoryID" access="public" returntype="void" output="false">		<cfargument name="SubCategoryID" type="string" required="true" />		<cfset variables.instance.SubCategoryID = arguments.SubCategoryID />	</cffunction>	<cffunction name="getSubCategoryID" access="public" returntype="string" output="false">		<cfreturn variables.instance.SubCategoryID />	</cffunction>	<!---	DUMP	--->	<cffunction name="dump" access="public" output="true" return="void">		<cfargument name="abort" type="boolean" default="false" />		<cfdump var="#variables.instance#" />		<cfif arguments.abort>			<cfabort />		</cfif>	</cffunction></cfcomponent>
