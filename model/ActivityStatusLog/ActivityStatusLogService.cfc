<cfcomponent name="ActivityStatusLogService" output="false">	<cfproperty name="ActivityStatusLogGateway" inject="model" scope="instance" />	<cfproperty name="ActivityStatusLogDAO" inject="model" scope="instance" />	<cfproperty name="ActivityStatusLog" inject="model" scope="instance" />				<cffunction name="init" access="public" output="false" returntype="ActivityStatusLogService">		<cfreturn this/>	</cffunction>	<cffunction name="new" access="public" output="false" returntype="ActivityStatusLog">				<cfreturn instance.ActivityStatusLog.init() />		</cffunction>			<cffunction name="createActivityStatusLog" access="public" output="false" returntype="ActivityStatusLog">		<cfargument name="ActivityStatusLogID" type="numeric" required="true" />		<cfargument name="ActivityID" type="numeric" required="false" />		<cfargument name="ActivityStatusID" type="numeric" required="false" />		<cfargument name="StatusDate" type="date" required="false" />		<cfargument name="logNote" type="string" required="false" />		<cfargument name="sysUserID" type="numeric" required="false" />							<cfset var ActivityStatusLog = createObject("component","ActivityStatusLog").init(argumentCollection=arguments) />		<cfreturn ActivityStatusLog />	</cffunction>	<cffunction name="getActivityStatusLog" access="public" output="false" returntype="ActivityStatusLog">		<cfargument name="ActivityStatusLogID" type="numeric" required="true" />				<cfset var ActivityStatusLog = createActivityStatusLog(argumentCollection=arguments) />		<cfset instance.ActivityStatusLogDAO.read(ActivityStatusLog) />		<cfreturn ActivityStatusLog />	</cffunction>	<cffunction name="getActivityStatusLogs" access="public" output="false" returntype="array">		<cfargument name="ActivityStatusLogID" type="numeric" required="false" />		<cfargument name="ActivityID" type="numeric" required="false" />		<cfargument name="ActivityStatusID" type="numeric" required="false" />		<cfargument name="StatusDate" type="date" required="false" />		<cfargument name="logNote" type="string" required="false" />		<cfargument name="sysUserID" type="numeric" required="false" />				<cfreturn instance.ActivityStatusLogGateway.getByAttributes(argumentCollection=arguments) />	</cffunction>	<cffunction name="getActivityStatusLogsQuery" access="public" output="false" returntype="query">		<cfargument name="ActivityStatusLogID" type="numeric" required="false" />		<cfargument name="ActivityID" type="numeric" required="false" />		<cfargument name="ActivityStatusID" type="numeric" required="false" />		<cfargument name="StatusDate" type="date" required="false" />		<cfargument name="logNote" type="string" required="false" />		<cfargument name="sysUserID" type="numeric" required="false" />		<cfargument name="orderby" type="string" required="false" />				<cfreturn instance.ActivityStatusLogGateway.getByAttributesQuery(argumentCollection=arguments) />	</cffunction>			<cffunction name="saveActivityStatusLog" access="public" output="false" returntype="boolean">		<cfargument name="ActivityStatusLog" type="ActivityStatusLog" required="true" />		<cfreturn instance.ActivityStatusLogDAO.save(ActivityStatusLog) />	</cffunction>	<cffunction name="deleteActivityStatusLog" access="public" output="false" returntype="boolean">		<cfargument name="ActivityStatusLogID" type="numeric" required="true" />				<cfset var ActivityStatusLog = createActivityStatusLog(argumentCollection=arguments) />		<cfreturn instance.ActivityStatusLogDAO.delete(ActivityStatusLog) />	</cffunction></cfcomponent>
